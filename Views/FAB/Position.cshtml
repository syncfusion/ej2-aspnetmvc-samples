@using Syncfusion.EJ2
@using Syncfusion.EJ2.Buttons
@section ControlsSection{
    <div class="control-section">
        <div id="target" class="fab-position-container custom-index">
            @Html.EJS().Fab("Fab1").Target("#target").HtmlAttributes(new Dictionary<string, object>() { { "title", "Top Left" } }).Position(FabPosition.TopLeft).IconCss("fab-icons fab-icon-people").CssClass("e-success").Render()
            @Html.EJS().Fab("Fab2").Target("#target").HtmlAttributes(new Dictionary<string, object>() { { "title", "Top Center" } }).Position(FabPosition.TopCenter).IconCss("fab-icons fab-icon-people").CssClass("e-warning").Render()
            @Html.EJS().Fab("Fab3").Target("#target").HtmlAttributes(new Dictionary<string, object>() { { "title", "Top Right" } }).Position(FabPosition.TopRight).IconCss("fab-icons fab-icon-people").CssClass("e-success").Render()
            @Html.EJS().Fab("Fab4").Target("#target").HtmlAttributes(new Dictionary<string, object>() { { "title", "Middle Left" } }).Position(FabPosition.MiddleLeft).IconCss("fab-icons fab-icon-people").CssClass("e-warning").Render()
            @Html.EJS().Fab("Fab5").Target("#target").HtmlAttributes(new Dictionary<string, object>() { { "title", "Middle Center" } }).Position(FabPosition.MiddleCenter).IconCss("fab-icons fab-icon-people").Render()
            @Html.EJS().Fab("Fab6").Target("#target").HtmlAttributes(new Dictionary<string, object>() { { "title", "Middle Right" } }).Position(FabPosition.MiddleRight).IconCss("fab-icons fab-icon-people").CssClass("e-warning").Render()
            @Html.EJS().Fab("Fab7").Target("#target").HtmlAttributes(new Dictionary<string, object>() { { "title", "Bottom Left" } }).Position(FabPosition.BottomLeft).IconCss("fab-icons fab-icon-people").CssClass("e-success").Render()
            @Html.EJS().Fab("Fab8").Target("#target").HtmlAttributes(new Dictionary<string, object>() { { "title", "Bottom Center" } }).Position(FabPosition.BottomCenter).IconCss("fab-icons fab-icon-people").CssClass("e-warning").Render()
            @Html.EJS().Fab("Fab9").Target("#target").HtmlAttributes(new Dictionary<string, object>() { { "title", "Bottom Right" } }).Position(FabPosition.BottomRight).IconCss("fab-icons fab-icon-people").CssClass("e-success").Render()
        </div>
    </div>
}

<style>
    .fab-position-container {
        position: relative;
        min-height: 310px;
        height: 400px;
        min-width: 100%;
    }
    .custom-index {
        z-index: 500 !important;
    }

    @@font-face {
        font-family: 'fab-icons';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }

    [class^="fab-icon-"],
    [class*=" fab-icon-"] {
        font-family: 'fab-icons' !important;
        speak: none;
        font-size: 55px;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: inherit;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    .fab-icon-people:before {
        content: "\e70a";
    }
</style>
@section Meta{
    <meta name="description" content="This example demonstrates the Positions in ASP.NET MVC FAB control. Explore here for more details." />
}
@section ActionDescription{
    <div id="action-description">
        <p>This sample demonstrates the different positions of the Floating Action Button in the target container. </p>
    </div>
}
@section Description{
    <div id="description">
        <p>
            Use the <code>Position</code> property to change the position of the FAB in the target element.
        </p>
    </div>
}
@section Title{
    <title>ASP.NET MVC FAB Positions Example - Syncfusion Demos </title>
}
@section Header{
    <h1 class='sb-sample-text'>Example of Positions in ASP.NET MVC FAB Control</h1>
}
